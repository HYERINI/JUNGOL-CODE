int main(void)
 
{
    int i, j;
    float score[6]  = { 85.6, 79.5, 83.1, 80.0, 78.2, 75.0 };
        scanf("%d %d", &i, &j);
    printf("%.1f", score[i-1] + score[j-1]);
}

배열1 자가진단 6번
#include <stdio.h>
int main(void)
{
    int num[10];
    int min = 1000;
    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &num[i]);
    }
    for (int j = 0; j < 10; j++)
    {
        if (min > num[j])
        {
            min = num[j];
        }
    }
    printf("%d", min);
}

배열1 자가진단 7번
#include <stdio.h>
int main(void)
{
    int num[10];
    int max = 1, min = 10000, a = 100;
     
    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &num[i]);
    }
     
    for (int j = 0; j < 10; j++)
    {
        if (num[j] < 100 && num[j] > max)
        {
            max = num[j];
        }
 
        else if (num[j] >= 100 && num[j] < min)
        {
            min = num[j];
        }
    }
    if (max == 1)
    {
        max = a;
    }
    if (min == 10000)
    {
        min = a;
    }
    printf("%d %d", max, min);
}

배열1 자가진단 8번
#include <stdio.h>
int main(void)
{
    int num[10];
    int sum = 0;
    double avg = 0;
 
    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &num[i]);
    }
 
    for (int j = 1;j < 10; j += 2)
    {
        sum += num[j];
    }
 
    for (int k = 0; k < 10; k += 2)
    {
        avg += num[k];
    }
 
    printf("sum : %d\navg : %.1f", sum, avg / 5);
 
}

배열1 자가진단 9번
#include <stdio.h>
int main(void)
{
    int num[10], array;
    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &num[i]);
    }
 
    for (int j = 0; j < 10; j++)
    {
        for (int k = 0; k < 9; k++)
        {
            if (num[k] < num[k + 1])
            {
                array = num[k];
                num[k] = num[k + 1];
                num[k + 1] = array;
            }
        }
    }
    for (int a = 0; a < 10; a++)
    {
        printf("%d ", num[a]);
    }
}

