출력 형성 평가 1번 
#include <stdio.h>
int main (void)
{
    printf("My name is Hong");
}

출력 형성 평가 2번
#include <stdio.h>
int main (void)
{
    printf("My hometown \nFlowering mountain");
}

출력 형성 평가 3번
#include <stdio.h>
int main (void)
{
    printf("TTTTTTTTTT \n");
    printf("TTTTTTTTTT \n");
    printf("    TT     \n");
    printf("    TT     \n");
    printf("    TT     \n");
}

출력 형성 평가 4번
#include <stdio.h>
int main (void)
{
    int kor = 90, mat = 80, eng = 100;
    printf("kor %d\n", kor);
    printf("mat %d\n", mat);
    printf("eng %d\n", eng);
    printf("sum %d\n", kor + mat + eng);
    printf("avg %d\n", (kor + mat + eng) / 3);
}

출력 형성 평가 5번
#include <stdio.h>
int main(void)
{
     
    printf("%15s%15s%15s \n", "Seoul", "10,312,545", "+91,375");
    printf("%15s%15s%15s \n", "Pusan", "3,567,910", "+5,868");
    printf("%15s%15s%15s \n", "Incheon", "2,758,296", "+64,888");
    printf("%15s%15s%15s \n", "Daegu", "2,511,676", "+17,230");
    printf("%15s%15s%15s \n", "Gwangju", "1,454,636", "+29,774");
     
}


