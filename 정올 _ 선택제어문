선택 제어문 자가진단 1번
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
    if (a < 0)
    {
        printf("%d\n", a);
        printf ( "minus");
    else  
    {
        printf("%d" , a);
    }
     
}

선택 제어문 자가진단 2번 
#include <stdio.h>
int main(void)
{
    int a, b, c;
    scanf("%d %d", &a, &b);
    c = b + 100 - a;
    printf("%d\n", c);
    if (c > 0)
    {
        printf("Obesity");
    }
}

선택 제어문 자가진단 3번
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
    if (a >= 20)
    {
        printf("adult");
    }
    else
    {
        printf("%d years later", 20 - a);
    }
}

