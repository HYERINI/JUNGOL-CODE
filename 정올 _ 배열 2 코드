배열2 자가진단 1번
#include <stdio.h>

int main(void) {

 char ch;
 int arr[26] = { 0 };

 while (1) {
  scanf(" %c", &ch);
  if (ch >= 'A' && ch <= 'Z')
   arr[ch - 65]++;
  else
   break;
 }

 for (int i = 0; i < 26; i++) {
  if (arr[i] != 0)
   printf("%c : %d \n", i + 65, arr[i]);
 }

 return 0;
}

배열2 자가진단 2번
#include "stdio.h"

int main(void)
{
	int arr[10]={0,};
	int num=0;
	int i=0,j=0,k=0;

	for (i = 0; i < 100; i++) {
		scanf("%d ", &num);
		if(num==0)  
			break;
		arr[num/10]++;
	}

	for(i=0 ; i<10 ; i++){
		if(arr[i]!=0){
			printf("%d : %d\n",i,arr[i]);
		}      
	}

	return 0;
}

배열2 자가진단 3번
#include "stdio.h"

int main(void)
{
	int arr[103]={0,};
	int ans = 0, k=0;

	scanf_s("%d", &ans);

	arr[0] = 100;
	arr[1] = ans;
	
	for (int i = 2; i < 100; i++) {
		arr[i]=arr[i-2]-arr[i-1];
	}

	for (int k = 0; k < 100; k++) {
		printf("%d ", arr[k]);
		if(arr[k]<0)	
		    break;
	}
	return 0;
}

