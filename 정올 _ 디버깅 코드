디버깅 자가진단 1번 
#include <stdio.h>
 
int main(void)
{
    double a, b;
    scanf("%lf %lf", &a, &b); 
    printf("%d %d", (int)(a * b), (int)a * (int)b);
}

디버깅 자가진단 2번
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
        printf("%d %.2lf", a / b, (double)(a) / b);
 
}

디버깅 형성평가 1번
#include <stdio.h>
int main(void)
{
    int a, b, c;
    double d;
    scanf("%d %d %d", &a, &b, &c);
    d = ((double)a + (double)b + (double)c) / 3;
    printf("%.1f", d);
}

디버깅 형성평가 2번
#include <stdio.h>
int main(void)
{
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);
    printf("sum %d\navg %d", (int)a + (int)b + (int)c, (int)((a + b + c) / 3));
}

디버깅 형성평가 3번
#include <stdio.h>
int main(void)
{
    int a = 5;
    a += 10;
    a = a - 1;
    printf("%d", a+1);
}
