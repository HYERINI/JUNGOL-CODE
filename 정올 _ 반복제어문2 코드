반복제어문2 자가진단 3번
#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
    for (int i = 1;i <= a; i++)
    {
        if(i % 2 == 0) {
            printf("%d ", i);
        }
        else {
            continue;
        }
    }
     
}

반복제어문2 자가진단 4번
#include <stdio.h>
int main(void)
{
    int a;
    int sum = 0;
    scanf("%d", &a);
    for (;a <= 100; a++)
    {
        sum += a;
    }
    printf("%d", sum);
}

반복제어문2 자가진단 5번
#include <stdio.h>
int main(void)
{
    int three = 0, five = 0;
    int a;
    for (int i = 0;i < 10;i++)
    {
        scanf("%d", &a);
        if (a % 3 == 0)
        {
            three += 1;
            if (a % 5 == 0)
            {
                five += 1;
            }
        }
        else if (a % 5 == 0)
        {
            five += 1;
        }
        else
        {
            continue;
        }
    }
    printf("Multiples of 3 : %d\nMultiples of 5 : %d", three, five);
}

반복제어문2 자가진단 6번
#include <stdio.h>
int main(void)
{
    int n, i, score;
    int sum = 0;
    scanf("%d", &n);
    for (i = 0;i < n;i++)
    {
        scanf("%d", &score);
        sum += score;
    }
    if (sum / n >= 80)
    {
        printf("avg : %.1lf\n", (double)(sum) / n);
        printf("pass");
    }
    else
    {
        printf("avg : %.1lf\n", (double)(sum) / n);
        printf("fail");
    }
}

반복제어문2 자가진단 7번
#include <stdio.h>
int main(void)
{
    int i, j = 0;
    int a = 1;
    for (i = 0;i < 5;i++)
    {
        a += i;
        a += 1; 
        for (j=0 ;j < 5; j++)
        {
             
            printf("%d ", a);
            a++;
        }
        a = 1;
         
 
        printf("\n");
    }
}

반복제어문2 자가진단 8번
#include <stdio.h>
int main(void)
{
    int i, a;
     
    for (i = 2; i < 5; i++)
    {
        for (a = 1; a < 6;a++)
        {
            printf("%d * %d =%3.d   ", i, a, i * a);
        }
        printf("\n");
    }
         
     
}

반복제어문2 형성평가 1번
#include <stdio.h>
int main(void)
{
    int i, n;
    scanf("%d", &n);
    for (i = 0; i < n;i++)
    {
        printf("JUNGOL\n");
    }
}

반복제어문2 형성평가 2번
#include <stdio.h>
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
    if (a < b)
    {
        for (;a <= b;a++)
        {
            printf("%d ", a);
        }
    }
    else {
        for (;b <= a;b++)
        {
            printf("%d ", b);
        }
    }
}

반복제어문2 형성평가 3번
#include <stdio.h>
int main(void)
{
    int a, i;
    int sum = 0;
    scanf("%d", &a);
    for (i = 1; i <= a; i++)
    {
        if (i % 5 == 0)
        {
            sum += i;
        }
        else
        {
            continue;
        }
    }
    printf("%d", sum);
}

반복제어문2 형성평가 4번
#include <stdio.h>
int main(void)
{
    int n, a;
    int sum = 0;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &a);
        sum += a;
    }
    printf("%.2lf", (double)(sum) / n);
}

반복제어문2 형성평가 5번
#include <stdio.h>
int main(void)
{
    int i, a;
    int even = 0, odd = 0;
    for (i = 0; i < 10; i++)
    {
        scanf("%d", &a);
        if (a % 2 == 0)
        {
            even += 1;
        }
        else
        {
            odd += 1;
        }
         
         
    }
    printf("even : %d\nodd : %d", even, odd);
}

반복제어문2 형성평가 6번
#include <stdio.h>
int main(void)
{
    int a, b,num = 0, sum = 0;
    scanf("%d %d", &a, &b);
    if (a < b) {
        for (;a <= b;a++)
        {
            if (a % 3 == 0 || a % 5 == 0)
            {
                sum += a;
                num += 1;
            }
            else
            {
                continue;
            }
        }
        printf("sum : %d\navg : %.1lf", sum, (double)(sum) / num);
    }
     
    else {
        for (;b <= a;b++)
        {
            if (b % 3 == 0 || b % 5 == 0)
            {
                sum += b;
                num += 1;
            }
            else
            {
                continue;
            }
        }
        printf("sum : %d\navg : %.1lf", sum, (double)(sum) / num);
    }
}

