int main(void)
{
    char text[10];
    {
        for (int i = 0; i < 10; i++)
        {
            scanf(" %c", &text[i]);
        }
        printf("%c ", text[0]);
        printf("%c ", text[3]);
        printf("%c ", text[6]);
    }
}

배열1 자가진단 4번
#include <stdio.h>
int main(void)
{
    int num[100];
    int i = 0;
    while (1)
    {
        scanf("%d", &num[i]);
        if (num[i] == 0)
            break;
        i++;
    }
 
    for (int j = i - 1; j >= 0; j--)
    {
        printf("%d ", num[j]);
    }
 
    return 0;
}

