    for (i = 1;i < 11;i++)
    {
        printf("%d ", a * i );
    }
 
}

반복제어문2 형성평가 8번
#include <stdio.h>
int main(void)
{
    int i , j ;
    int a, b;
    scanf("%d %d", &a, &b);
    for (i = 1;i <= a;i++)
    {
        for (j = 1; j <= b; j++)
        {
            printf("%d ", i * j);
        }
        printf("\n");
    }
}

반복제어문2 형성평가 9번
#include <stdio.h>
int main(void)
{
    int a, i;
    scanf("%d", &a);
    for (i = 1;i <= a;i++)
    {
        for (int j = 1;j <= a;j++)
        {
            printf("(%d, %d) ", i, j);
        }
        printf("\n");
    }
}

반복제어문2 형성평가 A번
#include <stdio.h>
int main(void)
{
    int a, b;
    int c;
    scanf("%d %d", &b, &a);
    if (b > a) {
        for (int i = 1;i <= 9; i++)
        {
            c = b;
            for (;c >= a;c--)
            {
                printf("%d * %d =%3.d   ", c, i, c * i);
            }
            printf("\n");
        }
    }
    else
    {
        for (int i = 1;i <= 9; i++)
        {
            c = b;
            for (;c <= a;c++)
            {
                printf("%d * %d =%3.d   ", c, i, c * i);
            }
            printf("\n");
        }
    }
}
